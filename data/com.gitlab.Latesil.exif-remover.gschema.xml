<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="exif-remover">
	<schema id="com.gitlab.Latesil.exif-remover" path="/com/gitlab/Latesil/exif-remover/">
	  <key type="i" name="folder-quantity">
      <default>0</default>
      <summary>Folder quantity</summary>
      <description>How many folders currently in the main screen</description>
    </key>
	  <key type="s" name="output-filename">
      <default>"image"</default>
      <summary>Output filename</summary>
      <description>Prefix for output files</description>
    </key>
	</schema>
</schemalist>
