<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="exif-remover">
	<schema id="com.github.Latesil.exif-remover" path="/com/github/Latesil/exif-remover/">
	  <key type="i" name="folder-quantity">
      <default>0</default>
      <summary>Folder quantity</summary>
      <description>How many folders currently in the main screen</description>
    </key>
	  <key type="s" name="output-filename">
      <default>"image"</default>
      <summary>Output filename</summary>
      <description>Prefix for output files</description>
    </key>
	  <key type="b" name="rename">
      <default>false</default>
      <summary>Should files be renamed</summary>
      <description>Determine whether files should be renamed. Renaming files is a process of adding quantity prefix at the end of the filename</description>
    </key>
	</schema>
</schemalist>
